<instance format="XCSP3" type="CSP">
  <variables>
    <array id="x" note="x[i] is the target node to which the ith pattern node is mapped" size="[180]"> 0..199 </array>
  </variables>
  <constraints>
    <allDifferent note="ensuring injectivity"> x[] </allDifferent>
    <group note="preserving edges">
      <extension>
        <list> %0 %1 </list>
        <supports> (0,2)(2,0) </supports>
      </extension>
      <args> x[0] x[1] </args>
      <args> x[1] x[8] </args>
    </group>
    <block class="redundant-constraints">
      <group>
        <extension>
          <list> %0 </list>
          <conflicts> 1 3..199 </conflicts>
        </extension>
        <args> x[0] </args>
        <args> x[8] </args>
      </group>
      <extension>
        <list> x[1] </list>
        <conflicts> 0..199 </conflicts>
      </extension>
    </block>
  </constraints>
</instance>
